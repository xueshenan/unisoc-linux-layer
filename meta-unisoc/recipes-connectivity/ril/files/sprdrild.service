[Unit]
Description=sprdrild Daemon Service
After=syslog.target
Before=ofono.service

[Service]
ExecStart=/usr/bin/sprdrild -l /usr/lib/libsprd-ril.so

#User=radio
#Group=radio
#SupplementaryGroups=inet net_admin net_raw
